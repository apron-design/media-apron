{
  "name": "@media-apron/vue",
  "version": "26.0.1",
  "description": "Media player components for Vue 3 - A branch project of Apron Design",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite example",
    "build": "npm run build:types && npm run build:lib",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:lib": "vite build",
    "type-check": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "vue3",
    "media",
    "video",
    "audio",
    "player",
    "component",
    "apron"
  ],
  "author": "https://apron.design/vue/",
  "homepage": "https://apron.design/vue/video",
  "license": "MIT",
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/tsconfig": "^0.5.0",
    "sass": "^1.97.1",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue": "^3.4.0",
    "vue-tsc": "^1.8.0"
  }
}
